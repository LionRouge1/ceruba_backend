<p style="color: green"><%= notice %></p>

<% content_for :title, "Websites" %>

<h1>Websites</h1>

<div id="websites">
  <% @websites.each do |website| %>
    <%= render website %>
    <p>
      <%= link_to "Show this website", website %>
    </p>
  <% end %>
</div>

<%= link_to "New website", new_website_path %>
